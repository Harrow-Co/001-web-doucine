# Variables d'environnement pour le frontend
VITE_API_URL=https://api.association-doucine.fr/api/v2

# Variables d'environnement pour le backend
NODE_ENV=production
PORT=3000

# Secret JWT pour l'authentification (générer une chaîne aléatoire sécurisée)
JWT_SECRET=your_secure_jwt_secret_here

# Variables pour Traefik (authentification du tableau de bord)
TRAEFIK_AUTH=admin:hashed_password_here

# Domaines pour les services
API_DOMAIN=api.association-doucine.fr
TRAEFIK_DOMAIN=traefik.association-doucine.fr

# Email pour Let's Encrypt (certificats SSL)
ACME_EMAIL=contact@harrowco.fr